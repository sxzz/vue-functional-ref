{
  "name": "vue-functional-ref",
  "version": "0.7.2",
  "packageManager": "pnpm@10.15.0",
  "description": "Functional-style refs for Vue",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/vue-functional-ref#readme",
  "bugs": {
    "url": "https://github.com/sxzz/vue-functional-ref/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/vue-functional-ref.git"
  },
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist",
    "types.d.ts"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./types.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./esbuild": "./dist/esbuild.js",
    "./rollup": "./dist/rollup.js",
    "./vite": "./dist/vite.js",
    "./types": "./types.d.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "unplugin-utils": "^0.3.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.1.4",
    "@sxzz/prettier-config": "^2.2.4",
    "@types/node": "^24.3.0",
    "@vue/reactivity": "^3.5.20",
    "@vue/shared": "^3.5.20",
    "bumpp": "^10.2.3",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "expect-type": "^1.2.2",
    "fast-glob": "^3.3.3",
    "prettier": "^3.6.2",
    "rollup": "^4.49.0",
    "tsdown": "^0.14.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.20"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "prettier": "@sxzz/prettier-config"
}
